            android:exported="false" />
        <activity
            android:name=".SettingScreenActivity"
            android:exported="false" />
        <activity
            android:name=".SearchScreenActivity"
            android:exported="false" />
        <activity
            android:name=".LocationScreenActivity"
            android:exported="false" />
        <activity
            android:name=".MaterialTypeActivity"
            android:exported="false" />
        <activity
            android:name=".MaterialActionActivity"
            android:exported="false" />
        <activity
            android:name=".MainMenuActivity"
            android:exported="false" />
        <activity
            android:name=".DummyActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity android:name=".SignUpActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.NayiDishaVyapar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>